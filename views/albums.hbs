<div class="around">
<div class="row row-cols-1 row-cols-md-5 g-4">
    {{#each album }}
    <div class="col">
    <div class="card">
      <img src="{{this.images.1.url}}" class="card-img-top" alt="Picture of Album">
      <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <a href="/tracks/{{this.id}}" class="btn btn-primary">View Tracks</a>
      </div>
    </div>
    </div>
    {{/each}}
</div>
</div>

